<!DOCTYPE HTML>
<html>
<head>
<title><%= title %></title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="content-type" content="text/html; charset=UTF-8">

<link rel="stylesheet" href="/layout.css">

</head>
<body>

<div id="dashboard">

<div id="left">

  <div>
  <canvas id="temp"></canvas>
  </div>

  <div>
  <canvas id="heat"></canvas>
  </div>

</div>

<div id="middle">

  <p> <img src="/desenho.svg" alt="Vapomatic" > </p>

  <div>
  <table>
  <tr><td>Ativo</td><td>Não</td></tr>
  <tr><td>Fan</td><td>Não</td></tr>
  <tr><td>Target</td><td>180</td></tr>
  <tr><td>Interna</td><td>180</td></tr>
  <tr><td>Externa</td><td>180</td></tr>
  <tr><td>Sonda</td><td>180</td></tr>
  <tr><td>Carga</td><td>180</td></tr>
  </table>
  </div>

  <div>
    <form id="prompt" action="/command">
    <p> <input type="text" name="command" id="command" placeholder="Comando"> </p>
    <p> <input type="submit" value="Amém"> </p>
    </form>
  </div>

</div>

<div id="right">

  <div>
  <canvas id="calib"></canvas>
  </div>

  <div>
  <canvas id="deriv"></canvas>
  </div>

</div>

</div> <!-- #dashboard -->

<script src="/chart.umd.js"></script>
<script src="/setup.js"></script>


</body>
</html>
